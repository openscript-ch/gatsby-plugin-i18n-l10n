{
  "name": "gatsby-plugin-i18n-l10n",
  "version": "5.7.0",
  "description": "Providing i18n and l10n to Gatsby with react-intl. Besides translating pages and labels, you can also translate the slugs and paths and still link between translated sibling pages.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-i18n-l10n",
    "i18n",
    "l10n",
    "intl",
    "internationalization",
    "translate",
    "react-intl"
  ],
  "homepage": "https://github.com/openscript-ch/gatsby-plugin-i18n-l10n",
  "repository": {
    "type": "git",
    "url": "https://github.com/openscript-ch/gatsby-plugin-i18n-l10n"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "author": "openscript GmbH <hi+npm@openscript.ch>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "check:format": "eslint . --max-warnings 0",
    "check:types": "tsc --noEmit",
    "dev:test": "jest --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "example": "npm run build && npm run --prefix ./examples",
    "postbuild": "node ./postbuild.js",
    "release:major": "standard-version --release-as major && git push --follow-tags",
    "release:minor": "standard-version --release-as minor && git push --follow-tags",
    "release:patch": "standard-version --release-as patch && git push --follow-tags",
    "release:create-github-release": "conventional-github-releaser -p angular",
    "test": "jest",
    "upgrade-interactive": "npm-check -u"
  },
  "peerDependencies": {
    "gatsby": "^5.x",
    "gatsby-source-filesystem": "^5.x",
    "react-intl": "^6.x|^7.x"
  },
  "peerDependenciesMeta": {
    "gatsby-source-filesystem": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.14.0",
    "conventional-github-releaser": "3.1.5",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gatsby": "^5.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-check": "6.0.1",
    "prettier": "^3.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intl": "^7.0.1",
    "react-test-renderer": "^18.3.1",
    "standard-version": "9.5.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "limax": "^4.1.0",
    "path-browserify": "^1.0.1"
  }
}
